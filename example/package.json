{
  "name": "@dr.pogodin/react-native-fs-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:prod": "react-native run-android --mode release",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "windows:autolink": "./node_modules/.bin/rnc-cli autolink-windows --sln \"windows\\ReactNativeFsExample.sln\" --proj \"windows\\ReactNativeFsExample\\ReactNativeFsExample.vcxproj\"",
    "build:android": "react-native build-android --extra-params \"--no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a\"",
    "build:ios": "react-native build-ios --mode Debug"
  },
  "dependencies": {
    "@dr.pogodin/react-native-static-server": "^0.24.2",
    "lodash": "^4.17.21",
    "react": "19.1.1",
    "react-native": "0.82.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-windows": "0.80.0-preview.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/runtime": "^7.28.4",
    "@react-native-community/cli": "20.0.2",
    "@react-native-community/cli-platform-android": "20.0.2",
    "@react-native-community/cli-platform-ios": "20.0.2",
    "@react-native/babel-preset": "0.82.0",
    "@react-native/eslint-config": "0.82.0",
    "@react-native/metro-config": "0.82.0",
    "@react-native/typescript-config": "0.82.0",
    "@types/lodash": "^4.17.20",
    "@types/react": "^19.1.10",
    "react-native-builder-bob": "^0.40.13",
    "react-native-monorepo-config": "^0.1.10"
  },
  "engines": {
    "node": ">=20"
  },
  "react-native-windows": {
    "init-windows": {
      "name": "ReactNativeFsExample",
      "namespace": "ReactNativeFsExample",
      "template": "old/uwp-cpp-app"
    }
  }
}
